{
  "name": "oas-client",
  "version": "1.0.0",
  "description": "TypeScript client for Open Atlas Search API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run typecheck && npm run test && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "search",
    "atlas",
    "mongodb",
    "typescript",
    "client"
  ],
  "author": "David Schrooten",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@types/node-fetch": "^2.6.11",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "files": [
    "dist/"
  ]
}
