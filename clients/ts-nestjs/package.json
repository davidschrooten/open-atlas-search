{
  "name": "oas-ts-nestjs-client",
  "version": "1.0.0",
  "description": "NestJS client for Open Atlas Search API built on oas-ts-client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run typecheck && npm run test && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "nestjs",
    "search",
    "atlas",
    "mongodb",
    "typescript",
    "client",
    "dependency-injection"
  ],
  "author": "David Schrooten",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "oas-ts-client": "^1.0.2"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/"
  ]
}
